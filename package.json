{
  "name": "@wayke-se/ecom-react",
  "version": "1.1.4",
  "description": "React components for wayke ecom.",
  "main": "src/index.tsx",
  "scripts": {
    "build": "pika build",
    "prebuild": "npm run clean",
    "build:tsc": "tsc",
    "prebuild:tsc": "npm run clean",
    "clean": "rimraf ./dist ./pkg",
    "lint": "tslint -c ./tslint.json -p ./tsconfig.json -e 'src/test/**/*' -e 'src/**/*.spec.{ts}' 'src/**/*.{ts,tsx}'",
    "prettify": "prettier --write 'src/**/*.{ts,tsx}'",
    "prepare": "npm run prettify && npm run type-check && npm run lint",
    "type-check": "tsc --noEmit --skipLibCheck"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wayke-se/wayke-ecom-react.git"
  },
  "keywords": [
    "wayke",
    "ecom",
    "react"
  ],
  "author": "Robin Sveningson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wayke-se/wayke-ecom-react/issues"
  },
  "homepage": "https://github.com/wayke-se/wayke-ecom-react#readme",
  "peerDependencies": {
    "react": "^16.8.0"
  },
  "engines": {
    "node": ">= 10.0.0"
  },
  "devDependencies": {
    "@pika/pack": "^0.5.0",
    "@pika/plugin-build-node": "^0.9.2",
    "@pika/plugin-build-types": "^0.9.2",
    "@pika/plugin-build-web": "^0.9.2",
    "@pika/plugin-copy-assets": "^0.9.2",
    "@pika/plugin-ts-standard-pkg": "^0.9.2",
    "@types/node": "^13.9.2",
    "@types/react": "^16.9.23",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "tslint": "^6.1.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.2.0",
    "tslint-react": "^4.2.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@wayke-se/ecom": "^1.0.8-alpha",
    "marked": "^0.8.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg"
      ],
      [
        "@pika/plugin-copy-assets"
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-build-web"
      ],
      [
        "@pika/plugin-build-types"
      ]
    ]
  }
}
