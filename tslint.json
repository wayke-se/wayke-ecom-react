{
  "defaultSeverity": "error",
  "extends": [
      "tslint:latest",
      "tslint-config-airbnb",
      "tslint-config-prettier",
      "tslint-react"
  ],
  "jsRules": {},
  "rules": {
    "import-name": [false],
    "no-implicit-dependencies": [true, "dev"],
    "object-literal-sort-keys": [false],
    "prefer-array-literal": [false]
  },
  "rulesDirectory": []
}
